{
    "name": "pixium/pixium-yii2tech-spreadsheet",
    "description": "Yii2 extension for export to a spreadsheet, e.g. Excel, LibreOffice etc.",
    "keywords": ["yii2", "spreadsheet", "excel", "libreoffice", "export", "xls", "xlsx"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/pixiumdigital/pixium-yii2tech-spreadsheet"
    },
    "authors": [
        {
            "name": "Remi Tache",
            "email": "remi.tache@pixiumdigital.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13",
        "phpoffice/phpspreadsheet": "~1.1",
        "kartik-v/yii2-grid": "^3.3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": { "pixium\\yii2tech-spreadsheet\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}